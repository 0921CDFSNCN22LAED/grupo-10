  <%- include("./partials/head") %>
  <body>
  <%- include("./partials/header") %>
    <main>
      <h2 class="titulo-micompra">
        <i class="fas fa-shopping-cart"></i>
        Mi Compra
      </h2>
      <h2 class="subtitulo-micompra">
        <i class="fas fa-dollar-sign"></i>
        Mi Pago
      </h2>
      <label for="label-efectivo">
        <input type="radio" name="label-pago" id="label-pago">
        <p>Efectivo</p>
      </label>
      <label for="label-transferencia">
        <input type="radio" name="label-pago" id="label-transferencia">
        <p>Transferencia bancaria</p>
      </label>
      <label for="label-tarjeta">
        <input type="radio" name="label-pago" id="label-tarjeta">
        <p>Tarjeta de crédito o débito</p>
      </label>
      <button type="submit" name="submit-anterior" id="submit-anterior">ANTERIOR</button>
      <button type="submit" name="submit-siguiente" id="submit-siguiente">SIGUIENTE</button>
    </main>
    <%- include("./partials/footer") %>
  </body>
