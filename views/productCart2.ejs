  <%- include("./partials/head", {title: "Carrito - PC Gamer"}) %>
  <body>
  <%- include("./partials/header") %>
    <main>
      <h2 class="titulo-micompra">
        <i class="fas fa-shopping-cart"></i>
        Mi Compra
      </h2>
      <h2 class="subtitulo-micompra">
        <i class="fas fa-truck"></i>
        Mi Entrega
      </h2>
      <label for="label-Pickup">
        <input type="radio" name="label-envio" id="label-Pickup">
        <p>PickUp Point</p>
        <p>(Retiro por Av. Independencia 3485)</p>
        <span class="">Gratis!</span>
      </label>
      <label for="label-envio">
        <input type="radio" name="label-envio" id="label-envio">
        <p>Envío a domicilio</p>
        <p>(Válido para todo el país)</p>
        <span class="">Calcular envío</span>
      </label>
      <button type="submit" name="submit-anterior" id="submit-anterior">ANTERIOR</button>
      <button type="submit" name="submit-siguiente" id="submit-siguiente">SIGUIENTE</button>
    </main>
    <%- include("./partials/footer") %>
  </body>
